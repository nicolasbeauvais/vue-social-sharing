    "babel-preset-es2015": "^6.1.18",
    "buble": "^0.13.1",
    "chromedriver": "^2.21.2",
    "conventional-changelog-cli": "^1.1.1",
    "conventional-github-releaser": "^1.1.3",
    "cross-spawn": "^4.0.0",
    "dotenv": "^2.0.0",
    "eslint": "^3.4.0",
